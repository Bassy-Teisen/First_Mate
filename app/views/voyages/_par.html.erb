

<%= form_with model: @voyage do |f| %>

    <p>
        <%= f.label :description %>
        <%= f.text_field :description %>
    </p>
    <p>
        <%= f.label :Launch_date %>
        <%= f.date_field :voyage %>
    </p>
    <p>
        <%= f.label :Launch_time %>
        <%= f.time_field :launch %>
    </p>
    <p>
        <%= f.label :voyage_image %>
        <%= f.file_field :voyage_image %>
    </p>
    <p>
        <%= f.submit 'Create new Voyage' %>
    </p>

<% end %>

    





