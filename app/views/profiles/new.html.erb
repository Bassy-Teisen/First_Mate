<h1>Profiles#new</h1>


<%= form_with model: @profiles do |f| %>
    <p>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :email %>
        <%= f.text_field :email %>
    </p>
    <p>
        <%= f.label :captain %>
        <%= f.text_field :captain %>
    </p>
    <p>
        <%= f.submit 'Create new profile' %>
    </p>
<% end %>
